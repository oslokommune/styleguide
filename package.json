{
  "name": "styleguide",
  "version": "0.45.0",
  "description": "Oslo Styleguide",
  "main": "index.js",
  "scripts": {
    "build:icons": "npm run build:icons:docs && npm run build:icons:font",
    "build:icons:font": "webfont \"src/assets/icons/**/*.svg\" --font-name 'OsloIcons' --formats 'woff','woff2' --font-height 512 --normalize --dest \"src/assets/fonts/\" --template \"src/atoms/icons/icon/_iconMap.scss.njk\" --dest-template \"src/assets/sass/common/\"",
    "build:icons:docs": "webfont \"src/assets/icons/**/*.svg\" --font-name 'OsloIcons' --formats 'woff','woff2' --font-height 512 --normalize --dest \"src/assets/fonts/\" --template \"src/atoms/icons/icon/icons.json.njk\" --dest-template \"src/globals/iconography/icons/\"",
    "commit": "cz",
    "release": "standard-version",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook --no-dll"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:oslokommune/styleguide.git"
  },
  "author": "Utviklings- og kompetanseetaten - Oslo kommune <devnull@uke.oslo.kommune.no>",
  "license": "proprietary",
  "homepage": "https://github.com/oslokommune/styleguide#readme",
  "dependencies": {
    "minireset.css": "0.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/addons": "^6.0.26",
    "@storybook/html": "^6.0.26",
    "@storybook/theming": "^6.0.26",
    "@storybook/vue": "^6.0.28",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^4.3.0",
    "node-sass": "^4.14.1",
    "react-is": "^17.0.1",
    "sass-loader": "^10.0.3",
    "standard-version": "^9.0.0",
    "style-loader": "^2.0.0",
    "styleguide_vue": "git+ssh://git@github.com/oslokommune/styleguide_vue.git#v0.0.10",
    "twig": "^1.15.2",
    "twig-loader": "^0.5.5",
    "vue": "^2.6.12",
    "vue-loader": "^15.9.5",
    "vue-template-compiler": "^2.6.12",
    "webfont": "^9.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
